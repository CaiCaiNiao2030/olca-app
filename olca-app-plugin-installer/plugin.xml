<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="%plugins.categorylabel"
            id="org.openlca.plugin.installer.commands.category">
      </category>
      <command
            name="%pluginmanager.commandlabel"
            categoryId="org.openlca.plugin.installer.commands.category"
            id="org.openlca.plugin.installer.commands.pluginManagerCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.openlca.plugin.installer.commands.pluginManagerCommand"
            class="org.openlca.plugin.installer.handlers.PluginManagerHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <!-- <key
            commandId="org.openlca.plugin.installer.commands.pluginManagerCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
      -->
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.openlca.core.application.settings.menu">
            <command
                  commandId="org.openlca.plugin.installer.commands.pluginManagerCommand"
                  mnemonic="%pluginmanager.commandmnemonic"
                  id="org.openlca.plugin.installer.menus.pluginManagerCommand" >
            </command>
      </menuContribution>
   </extension>

</plugin>
