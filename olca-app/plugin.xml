<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.openlca.core.application.editors" name="Editors" schema="schema/org.openlca.core.application.editors.exsd"/>
   <extension-point id="org.openlca.core.application.exportAction" name="org.openlca.core.application.exportAction" schema="schema/org.openlca.core.application.exportAction.exsd"/>
   <extension-point id="org.openlca.core.application.importAction" name="org.openlca.core.application.importAction" schema="schema/org.openlca.core.application.importAction.exsd"/>
   <extension-point id="org.openlca.core.application.plugin" name="openLCA Plugin" schema="schema/org.openlca.core.application.plugin.exsd"/>

	<extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.openlca.core.editors.actor.ActorEditor"
            default="false"
            icon="icons/actor_obj.png"
            id="org.openlca.core.editors.actor.ActorEditor"
            name="%editor.name">
      </editor>
      <editor
            class="org.openlca.core.editors.source.SourceEditor"
            default="false"
            icon="icons/source_obj.png"
            id="org.openlca.core.editors.source.SourceEditor"
            name="%editor.name.0">
      </editor>
      <editor
            class="org.openlca.core.editors.unitgroup.UnitGroupEditor"
            default="false"
            icon="icons/unitgroup_obj.gif"
            id="org.openlca.core.editors.unitgroup.UnitGroupEditor"
            name="%editor.name.1">
      </editor>
      <editor
            class="org.openlca.core.editors.flowproperty.FlowPropertyEditor"
            default="false"
            icon="icons/flowprop_obj.gif"
            id="org.openlca.core.editors.flowproperty.FlowPropertyEditor"
            name="%editor.name.2">
      </editor>
      <editor
            class="org.openlca.core.editors.flow.FlowEditor"
            default="false"
            icon="icons/flow_obj.gif"
            id="org.openlca.core.editors.flow.FlowEditor"
            name="%editor.name.3">
      </editor>
      <editor
            class="org.openlca.core.editors.lciamethod.LCIAMethodEditor"
            default="false"
            icon="icons/LCIA_obj.gif"
            id="org.openlca.core.editors.lciamethod.LCIAMethodEditor"
            name="%editor.name.7">
      </editor>
      <editor
            class="org.openlca.core.editors.process.ProcessEditor"
            default="false"
            icon="icons/proc_obj.gif"
            id="org.openlca.core.editors.process.ProcessEditor"
            name="%editor.name.4">
      </editor>
      <editor
            class="org.openlca.core.editors.project.ProjectEditor"
            default="false"
            icon="icons/project_obj.png"
            id="org.openlca.core.editors.project.ProjectEditor"
            name="%editor.name.5">
      </editor>
      <editor
            class="org.openlca.core.editors.productsystem.ProductSystemEditor"
            contributorClass="org.openlca.core.editors.productsystem.graphical.GraphicalEditorActionBarContributor"
            default="false"
            icon="icons/model.gif"
            id="org.openlca.core.editors.productsystem.ProductSystemEditor"
            name="%editor.name.6">
      </editor>
      <editor
            class="org.openlca.core.editors.result.ResultEditor"
            contributorClass="org.openlca.core.editors.result.ResultActionProvider"
            default="false"
            icon="icons/expression_obj.gif"
            id="org.openlca.core.editors.result.ResultEditor"
            name="Result viewer">
      </editor>
      <editor
            class="org.openlca.core.editors.analyze.AnalyzeEditor"
            contributorClass="org.openlca.core.editors.analyze.AnalyzeActionContributor"
            default="false"
            icon="icons/analyze.gif"
            id="org.openlca.core.editors.analyze.AnalyzeEditor"
            name="Analyze viewer">
      </editor>
      <editor
            class="org.openlca.core.editors.productsystem.SimulationEditor"
            default="false"
            icon="icons/simulate_16.png"
            id="SimulationEditor"
            name="Simulation Editor">
      </editor>      
      <editor
            class="org.openlca.core.editors.result.CostResultEditor"
            default="false"
            icon="icons/simulate_16.png"
            id="CostResultEditor"
            name="Cost results">
      </editor>      
   </extension>
   
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            class="org.openlca.core.editors.actor.ActorWizard"
            icon="icons/actor_obj_new.png"
            id="newActorWizard"
            name="%wizard.name">
      </wizard>
      <wizard
            class="org.openlca.core.editors.source.SourceWizard"
            icon="icons/source_obj_new.png"
            id="newSourceWizard"
            name="%wizard.name.0">
      </wizard>
      <wizard
            class="org.openlca.core.editors.unitgroup.UnitGroupWizard"
            icon="icons/unitgroup_obj_new.gif"
            id="newUnitGroupWizard"
            name="%wizard.name.1">
      </wizard>
      <wizard
            class="org.openlca.core.editors.flowproperty.FlowPropertyWizard"
            icon="icons/flowprop_obj_new.gif"
            id="newFlowPropertyWizard"
            name="%wizard.name.2">
      </wizard>
      <wizard
            class="org.openlca.core.editors.flow.FlowWizard"
            icon="icons/flow_obj_new.gif"
            id="newFlowWizard"
            name="%wizard.name.3">
      </wizard>
      <wizard
            class="org.openlca.core.editors.lciamethod.LCIAMethodWizard"
            icon="icons/LCIA_obj_new.gif"
            id="newImpactMethodWizard"
            name="%wizard.name.7">
      </wizard>
      <wizard
            class="org.openlca.core.editors.process.ProcessWizard"
            icon="icons/proc_obj_new.gif"
            id="newProcessWizard"
            name="%wizard.name.4">
      </wizard>
      <wizard
            class="org.openlca.core.editors.project.ProjectWizard"
            icon="icons/project_obj_new.png"
            id="newProjectWizard"
            name="%wizard.name.5">
      </wizard>
      <wizard
            class="org.openlca.core.editors.productsystem.ProductSystemWizard"
            icon="icons/chart_organisation_new.png"
            id="newProductSystemWizard"
            name="%wizard.name.6">
      </wizard>
   </extension>   
   
   <extension
         id="org.openlca.core.application.application"
         name="Search"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.openlca.core.application.plugin.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="%perspective.name"
            class="org.openlca.core.application.plugin.Perspective"
            id="org.openlca.core.application.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="org.openlca.core.application.views.navigator.Navigator"
            icon="icons/categories.gif"
            id="org.openlca.core.application.navigator"
            name="%view.name">
      </view>  
 	  <view
            class="org.openlca.core.application.views.search.SearchView"
            icon="icons/search_obj.png"
            id="org.openlca.core.application.views.SearchView"
            name="%view.name.0"
            restorable="true">
      </view> 
   </extension>
   
   <extension
         point="org.eclipse.ui.editors">
      <editor
            default="false"
            id="application.views.UsageView"
            name="%usage"
            class="org.openlca.core.application.views.UsageView"
            icon="icons/16x16_link_blue.png">
      </editor>
   </extension>
 
   <extension
         point="org.eclipse.ui.preferencePages">
	  <page
            class="org.openlca.core.application.preferencepages.LocationPreferencePage"
            id="org.openlca.core.application.preferencepages.LocationPreferencePage"
            name="%page.name.2">
      </page>
      <page
            class="org.openlca.core.application.preferencepages.DatabaseParametersPreferencePage"
            id="org.openlca.core.application.preferencepages.DatabaseParametersPreferencePage"
            name="%page.name.1">
      </page>
      <page
            class="org.openlca.core.application.preferencepages.CalculationPreferencePage"
            id="org.openlca.core.application.preferencepages.CalculationPreferencePage"
            name="%page.name.3">
      </page>
      <page
            class="org.openlca.core.application.logging.LoggerConfigPage"
            id="org.openlca.core.application.logging.LoggerConfigPage"
            name="%logger.config.page.name">
      </page>
      <page
      		class="org.openlca.core.application.preferencepages.NumberFormatPage"
      		id="NumberFormatPage"
      		name="%number.format.page.name">
      </page>
      <page
            class="org.openlca.core.application.update.UpdatePreferencePage"
            id="org.openlca.core.application.update.UpdatePreferencePage"
            name="%update.preference.page.name">
      </page>
      <page
            class="org.openlca.core.application.preferencepages.FeatureFlagPage"
            id="org.openlca.core.application.preferencepages.FeatureFlagPage"
            name="Experimental features">
      </page>
   </extension>
   
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="openLCA Updates"
            id="org.openlca.core.application.update.commands.category">
      </category>
      <command
            name="%updatecheck.commandlabel"
            categoryId="org.openlca.core.application.update.commands.category"
            id="org.openlca.core.application.update.commands.updateCheckCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.openlca.core.application.update.commands.updateCheckCommand"
            class="org.openlca.core.application.update.UpdateCheckHandler">
      </handler>
   </extension>
   
    <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?before=help">
         <menu
               label="Settings"
               mnemonic="S"
               id="org.openlca.core.application.settings.menu">
            <command
                  commandId="org.openlca.core.application.update.commands.updateCheckCommand"
                  mnemonic="%updatecheck.commandmnemonic"
                  id="org.openlca.core.application.update.menus.updateCheckCommand">
            </command>
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  id="org.openlca.core.application.settings.menu.preferencesSettinsEntry">
            </command>
         </menu>
      </menuContribution>
   </extension>

   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="context.xml"
            plugin="org.openlca.core.application">
      </contexts>
   </extension>
   <extension
         id="openLCA"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.openlca.core.application.application"
            name="openLCA">
         <property
               name="windowImages"
               value="icons/logo_16_32bit.png,icons/logo_32_32bit.png,icons/logo_48_32bit.png,icons/logo_64.png,icons/logo_128.png">
         </property>
         <property
               name="aboutText"
               value="openLCA 1.3.0&#x0A;&#x0A;Copyright (c) 2007 - 2013 GreenDelta. All rights reserved.&#x0A;Visit http://openlca.org and http://greendelta.com&#x0A;&#x0A;This product includes software developed in the openLCA &#x0A;project and other open source software.">
         </property>
         <property
               name="aboutImage"
               value="icons/about.png">
         </property>
         <property
               name="appName"
               value="openLCA">
         </property>
         <property
               name="introBrandingImage"
               value="product:icons/logo_glowing.png" />
         <property
               name="introBrandingImageText"
               value="openLCA" />
         <property
               name="introTitle"
               value="%introTitle" />
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="org.openlca.core.application.navigator">
      </viewer>
      <viewerContentBinding
            viewerId="org.openlca.core.application.navigator">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="org.openlca.core.application.navigationContext">
            </contentExtension>
            <contentExtension
                  pattern="org.openlca.core.application.views.navigator.filter.EmptyCategoryFilter">
            </contentExtension>
            <contentExtension
                  pattern="org.openlca.core.application.views.navigator.filter.ProductFlowFilter">
            </contentExtension>
            <contentExtension
                  pattern="org.openlca.core.application.views.navigator.filter.ElementaryFlowFilter">
            </contentExtension>
            <contentExtension
                  pattern="org.openlca.core.application.views.navigator.filter.WasteFlowFilter">
            </contentExtension>
            <contentExtension
                  pattern="org.openlca.core.application.views.navigator.filter.UnitProcessFilter">
            </contentExtension>
            <contentExtension
                  pattern="org.openlca.core.application.views.navigator.filter.SystemProcessFilter">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="org.openlca.core.application.navigator">
         <includes>
            <actionExtension
                  pattern="org.openlca.core.application.navigator.actionProvider">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <dragAssistant
            class="org.openlca.core.application.views.navigator.NavigationDragAssistant"
            viewerId="org.openlca.core.application.navigator">
      </dragAssistant>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.openlca.core.application.navigation.NavigationContentProvider"
            id="org.openlca.core.application.navigationContext"
            labelProvider="org.openlca.core.application.navigation.NavigationLabelProvider"
            name="%navigatorContent.name"
            priority="normal">
         <triggerPoints>
            <instanceof
                  value="org.openlca.core.application.navigation.INavigationElement">
            </instanceof>
         </triggerPoints>
         <commonSorter
               class="org.openlca.core.application.navigation.NavigationSorter">
         </commonSorter>
         <dropAssistant
               class="org.openlca.core.application.views.navigator.NavigationDropAssistant"
               id="org.openlca.core.application.dropAssistant">
            <possibleDropTargets>
               <instanceof
                     value="org.openlca.core.application.navigation.INavigationElement">
               </instanceof>
            </possibleDropTargets>
         </dropAssistant>
         <possibleChildren>
            <instanceof
                  value="org.openlca.core.application.navigation.INavigationElement">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            class="org.openlca.core.application.navigation.NavigationActionProvider"
            id="org.openlca.core.application.navigator.actionProvider"
            priority="normal">
         <enablement></enablement>
      </actionProvider>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.core.application.views.navigator.filter.EmptyCategoryFilter"
            id="org.openlca.core.application.views.navigator.filter.EmptyCategoryFilter"
            name="%commonFilter.name">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.core.application.views.navigator.filter.ProductFlowFilter"
            id="org.openlca.core.application.views.navigator.filter.ProductFlowFilter"
            name="%commonFilter.name.0">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.core.application.views.navigator.filter.ElementaryFlowFilter"
            id="org.openlca.core.application.views.navigator.filter.ElementaryFlowFilter"
            name="%commonFilter.name.1">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.core.application.views.navigator.filter.WasteFlowFilter"
            id="org.openlca.core.application.views.navigator.filter.WasteFlowFilter"
            name="%commonFilter.name.2">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.core.application.views.navigator.filter.UnitProcessFilter"
            id="org.openlca.core.application.views.navigator.filter.UnitProcessFilter"
            name="%commonFilter.name.3">
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="org.openlca.core.application.views.navigator.filter.SystemProcessFilter"
            id="org.openlca.core.application.views.navigator.filter.SystemProcessFilter"
            name="%commonFilter.name.4">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.openlca.core.application.logging.LoggerPreference">
      </initializer>
      <initializer
            class="org.openlca.core.application.Preferences">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.openlca.core.application.update.UpdatePreference">
      </initializer>
   </extension>
   
</plugin>
