mixin header()
	header
		.row
			.left
				a(href='http://www.openlca.org')
					img.app-logo(src='images/logo_start_page.png')
			.center
				h1!=msg.title
				h2#version
		.row
			.right
				a(href='http://www.greendelta.com')
					img.company-logo(src='images/greendelta-logo.png')
			.center
				p!=msg.subtext
		.clear
